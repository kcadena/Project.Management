<Window x:Name="login_window" x:Class="MProjectWPF.UserControl.Inicio.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MProjectWPF.UserControl.Inicio"
        mc:Ignorable="d"
        Title="Inicio Sesion" Width="300" Height="416" >
    <Grid>
        <Label x:Name="label" Content="Usuario" Margin="75,93,97,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" Height="23" Margin="75,123.96,73,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Copy" Margin="75,0,73,164.54" TextWrapping="Wrap" Height="23" VerticalAlignment="Bottom"/>
        <Label x:Name="label_Copy" Content="Contraseña" Margin="75,166.5,97,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Registrarse" HorizontalAlignment="Left" Margin="76.59,0,0,130" VerticalAlignment="Bottom" Foreground="#FF2D90C9" MouseLeftButtonUp="label1_MouseLeftButtonUp"/>
        <Button x:Name="button" Content="Iniciar Sesion" Margin="80,0,137,100" VerticalAlignment="Bottom" Click="button_Click"/>

    </Grid>
</Window>
