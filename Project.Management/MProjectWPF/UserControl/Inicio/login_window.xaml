<Window x:Name="login_window" x:Class="MProjectWPF.UserControl.Inicio.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MProjectWPF.UserControl.Inicio"
        mc:Ignorable="d"
        Title="Inicio Sesion" Width="300" Height="416" >
    <Grid>
        <Label x:Name="label" Content="Usuario" Margin="10,23,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120"/>
        <TextBox x:Name="textBox" Height="23" Margin="10,53.96,138,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Copy" Margin="10,127.46,138,0" TextWrapping="Wrap" Height="23" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Contraseña" Margin="10,96.5,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120"/>
        <Label x:Name="label1" Content="Registrarse" HorizontalAlignment="Left" Margin="10,0,0,141.03" VerticalAlignment="Bottom" Foreground="#FF2D90C9"/>
        <Button x:Name="button" Content="Iniciar Sesion" HorizontalAlignment="Right" Margin="0,0,45.5,149.04" VerticalAlignment="Bottom" Width="75"/>

    </Grid>
</Window>
